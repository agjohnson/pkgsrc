     if ".$can_build_shared" = .no
     then ZZIPLIB_LDFLAGS=""
-    else ZZIPLIB_LDFLAGS="--export-dynamic"
+    else ZZIPLIB_LDFLAGS="${EXPORT_SYMBOLS_LDFLAGS}"
     fi
     RESOLVES=' # '
